local a="teleportPos.txt"local b=game:GetService("StarterGui")local c=game:GetService("UserInputService")local d=game:GetService("Players")local function e(f,g,h)b:SetCore("SendNotification",{Title=f,Text=g,Duration=h or 3})end;local function i()local j=d.LocalPlayer;return j.Character and j.Character:FindFirstChild("HumanoidRootPart")end;local function k(l)if isfile(a)and not l then e("üìç Coordenada j√° salva!","Use SHIFT+G para sobrescrever.",4)return end;local m=i()if m then local n=m.Position;writefile(a,n.X..","..n.Y..","..n.Z)e("‚úÖ Sucesso!","Posi√ß√£o salva com sucesso.",3)end end;local function o()if not isfile(a)then e("‚ö†Ô∏è Erro","Nenhuma coordenada salva encontrada!",3)return end;local p=readfile(a)local x,y,z=p:match("([^,]+),([^,]+),([^,]+)")if not(x and y and z)then e("‚ö†Ô∏è Erro","Arquivo de coordenadas inv√°lido!",3)return end;local m=i()if m then m.CFrame=CFrame.new(tonumber(x),tonumber(y),tonumber(z))e("üöÄ Teleporte","X="..x.." | Y="..y.." | Z="..z,4)end end;c.InputBegan:Connect(function(q,r)if r then return end;local s=q.KeyCode;local t=c:IsKeyDown(Enum.KeyCode.LeftShift)if s==Enum.KeyCode.G then k(t)elseif s==Enum.KeyCode.H then o()end end)e("üìå Controles","G = Salvar | SHIFT+G = Sobrescrever | H = Teleportar",6)
